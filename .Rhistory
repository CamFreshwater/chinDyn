# percentage of the mean is included in 95% CI
estCI <- function(n., logMu, logVar) {
mean. <- logMu + (logVar / 2)
int. <- 2.093 * (sqrt((logVar / n.) + ((logVar^2) / (2 * n. - 1))))
low <- exp(mean. - int.) / mean.
high <- exp(mean. + int.) / mean.
list(low, high)
}
